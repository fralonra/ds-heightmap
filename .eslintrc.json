{
  "extends": "standard",
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["warn", 2, {
      "SwitchCase": 1
    }],
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "no-console": 0,
    "no-empty": 2,
    "no-eq-null": 2,
    "no-new": 0,
    "no-fallthrough": 0,
    "no-unreachable": 0,
    "no-unused-vars": 1,
    "no-path-concat": 0,
    "one-var": 0,
    "operator-linebreak": 0,
    "space-before-function-paren": 1,
    "spaced-comment": 1
  }
}
